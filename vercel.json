{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
    ]
    }
  ],
  "buildCommand": "nest build && npx prisma generate && npx prisma migrate deploy"
}